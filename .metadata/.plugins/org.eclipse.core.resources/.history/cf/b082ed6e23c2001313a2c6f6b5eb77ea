function start(){
	var name = document.getElementById('name').value;
	var pass = document.getElementById('pass').value;
	
	var key = 0;
	document.write("Name: " + name + "<br>");
	document.write("Pass: " + pass + "<br>");
	
	update = function() {
		document.addEventListener('keydown', function(event) {
			key  = event.keyCode;
		}, true);
		
		document.write(key);
	};
}
